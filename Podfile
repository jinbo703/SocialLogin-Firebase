# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SocialLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SocialLogin
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  pod 'Firebase/Auth'
  pod 'GoogleSignIn'

  pod 'Fabric'
  pod 'TwitterKit'

end
